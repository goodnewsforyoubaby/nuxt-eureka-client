  "license": "MIT",
  "contributors": [
    {
      "name": "Kirill Urazmanov"
    }
  ],
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "types": "dist",
  "scripts": {
    "dev": "nuxt example",
    "lint": "eslint --ext .js,.vue .",
