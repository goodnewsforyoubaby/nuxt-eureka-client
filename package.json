    "@nuxtjs/module-test-utils": "latest",
    "@types/eureka-js-client": "^4.5.5",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "latest",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "latest",
    "lint-staged": "^12.4.1",
    "nuxt-edge": "latest",
    "prettier": "latest",
    "standard-version": "latest",
    "typescript": "^4.6.4"
